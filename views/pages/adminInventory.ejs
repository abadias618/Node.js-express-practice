<!DOCTYPE html>
<html>

<head>
    <%-include('../partials/head');%>
    <link rel="stylesheet" href="../stylesheets/adminInventory.css">
</head>

<body>
    <main>
        <%-include('../partials/header');%>
    </main>
    <div id="table">
        <table>
            <tr>
            <%for(var column in%> 
            <%=result.fields%>
            <%){%>    
                <td><label><%=result.fields[column].name%></label></td>
            </tr>
            <%}%>

            <%for(var row in %> 
            <%=result.rows%>
            <%){%>
            <tr>
                <%for(var column in %>
                <%=result.rows[row]%>
                <%){%>
                <td>
                    <label>
                        <%=result.rows[row][column]%>
                    </label>
                </td>
                <%}%>
            </tr>
            <%}%>
        </table>
    </div>
    
    <footer>
        <%- include('../partials/footer');%>
    </footer>
</body>

</html>